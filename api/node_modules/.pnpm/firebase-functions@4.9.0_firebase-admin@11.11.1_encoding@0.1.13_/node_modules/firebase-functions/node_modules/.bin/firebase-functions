#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/z/work/hanzo/backend/node_modules/.pnpm/firebase-functions@4.9.0_firebase-admin@11.11.1_encoding@0.1.13_/node_modules/firebase-functions/lib/bin/node_modules:/Users/z/work/hanzo/backend/node_modules/.pnpm/firebase-functions@4.9.0_firebase-admin@11.11.1_encoding@0.1.13_/node_modules/firebase-functions/lib/node_modules:/Users/z/work/hanzo/backend/node_modules/.pnpm/firebase-functions@4.9.0_firebase-admin@11.11.1_encoding@0.1.13_/node_modules/firebase-functions/node_modules:/Users/z/work/hanzo/backend/node_modules/.pnpm/firebase-functions@4.9.0_firebase-admin@11.11.1_encoding@0.1.13_/node_modules:/Users/z/work/hanzo/backend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/z/work/hanzo/backend/node_modules/.pnpm/firebase-functions@4.9.0_firebase-admin@11.11.1_encoding@0.1.13_/node_modules/firebase-functions/lib/bin/node_modules:/Users/z/work/hanzo/backend/node_modules/.pnpm/firebase-functions@4.9.0_firebase-admin@11.11.1_encoding@0.1.13_/node_modules/firebase-functions/lib/node_modules:/Users/z/work/hanzo/backend/node_modules/.pnpm/firebase-functions@4.9.0_firebase-admin@11.11.1_encoding@0.1.13_/node_modules/firebase-functions/node_modules:/Users/z/work/hanzo/backend/node_modules/.pnpm/firebase-functions@4.9.0_firebase-admin@11.11.1_encoding@0.1.13_/node_modules:/Users/z/work/hanzo/backend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../lib/bin/firebase-functions.js" "$@"
else
  exec node  "$basedir/../../lib/bin/firebase-functions.js" "$@"
fi
