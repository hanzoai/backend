#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/z/work/hanzo/backend/node_modules/.pnpm/firebase-tools@13.13.3_encoding@0.1.13/node_modules/firebase-tools/lib/bin/node_modules:/Users/z/work/hanzo/backend/node_modules/.pnpm/firebase-tools@13.13.3_encoding@0.1.13/node_modules/firebase-tools/lib/node_modules:/Users/z/work/hanzo/backend/node_modules/.pnpm/firebase-tools@13.13.3_encoding@0.1.13/node_modules/firebase-tools/node_modules:/Users/z/work/hanzo/backend/node_modules/.pnpm/firebase-tools@13.13.3_encoding@0.1.13/node_modules:/Users/z/work/hanzo/backend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/z/work/hanzo/backend/node_modules/.pnpm/firebase-tools@13.13.3_encoding@0.1.13/node_modules/firebase-tools/lib/bin/node_modules:/Users/z/work/hanzo/backend/node_modules/.pnpm/firebase-tools@13.13.3_encoding@0.1.13/node_modules/firebase-tools/lib/node_modules:/Users/z/work/hanzo/backend/node_modules/.pnpm/firebase-tools@13.13.3_encoding@0.1.13/node_modules/firebase-tools/node_modules:/Users/z/work/hanzo/backend/node_modules/.pnpm/firebase-tools@13.13.3_encoding@0.1.13/node_modules:/Users/z/work/hanzo/backend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../lib/bin/firebase.js" "$@"
else
  exec node  "$basedir/../../lib/bin/firebase.js" "$@"
fi
