#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/z/work/hanzo/backend/node_modules/.pnpm/pretty-quick@4.0.0_prettier@3.3.3/node_modules/pretty-quick/lib/node_modules:/Users/z/work/hanzo/backend/node_modules/.pnpm/pretty-quick@4.0.0_prettier@3.3.3/node_modules/pretty-quick/node_modules:/Users/z/work/hanzo/backend/node_modules/.pnpm/pretty-quick@4.0.0_prettier@3.3.3/node_modules:/Users/z/work/hanzo/backend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/z/work/hanzo/backend/node_modules/.pnpm/pretty-quick@4.0.0_prettier@3.3.3/node_modules/pretty-quick/lib/node_modules:/Users/z/work/hanzo/backend/node_modules/.pnpm/pretty-quick@4.0.0_prettier@3.3.3/node_modules/pretty-quick/node_modules:/Users/z/work/hanzo/backend/node_modules/.pnpm/pretty-quick@4.0.0_prettier@3.3.3/node_modules:/Users/z/work/hanzo/backend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../lib/cli.mjs" "$@"
else
  exec node  "$basedir/../../lib/cli.mjs" "$@"
fi
